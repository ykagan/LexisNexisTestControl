<?xml version="1.0" encoding="utf-8"?>
<wsdl:definitions xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://ws.identityproofing.idm.risk.lexisnexis.com/" xmlns:ns1="http://cxf.apache.org/bindings/xformat" name="identityProofingService" targetNamespace="http://ws.identityproofing.idm.risk.lexisnexis.com/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
  <wsdl:types>
    <xsd:schema xmlns:survey="http://ns.lexisnexis.com/survey/1.0" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://ns.lexisnexis.com/survey/1.0" version="9.0">
      <xsd:complexType name="Survey">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="1" name="status" type="survey:SurveyProductStatus" />
          <xsd:choice>
            <xsd:element name="questions" type="survey:SurveyQuestions" />
            <xsd:element name="surveyMessage" type="survey:SurveyMessage" />
          </xsd:choice>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="SurveyProductStatus">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="PENDING" />
          <xsd:enumeration value="COMPLETED" />
          <xsd:enumeration value="SYSTEM_ERROR" />
          <xsd:enumeration value="NOT_ENOUGH_DATA" />
          <xsd:enumeration value="SERVICE_UNAVAILABLE" />
          <xsd:enumeration value="ABANDONED" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="SurveyQuestions">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="numQuestions" type="xsd:int" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="question" type="survey:SurveyQuestion" />
        </xsd:sequence>
        <xsd:attribute name="surveyId" type="xsd:string" />
      </xsd:complexType>
      <xsd:complexType name="SurveyQuestion">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="text" type="xsd:string" />
          <xsd:element maxOccurs="unbounded" name="choice" type="survey:SurveyChoice" />
        </xsd:sequence>
        <xsd:attribute name="questionId" type="xsd:string" use="required" />
        <xsd:attribute name="sequenceId" type="xsd:int" use="required" />
        <xsd:attribute name="minRequiredChoices" type="xsd:int" use="optional" />
        <xsd:attribute name="maxRequiredChoices" type="xsd:int" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="SurveyChoice">
        <xsd:sequence>
          <xsd:element name="text" type="xsd:string" />
        </xsd:sequence>
        <xsd:attribute name="choiceId" type="xsd:string" />
        <xsd:attribute name="sequenceId" type="xsd:int" />
      </xsd:complexType>
      <xsd:complexType name="SurveyMessage">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="text">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="Survey answers were saved." />
                <xsd:enumeration value="All questions were not answered." />
                <xsd:enumeration value="Some questions do not have correct number of answers." />
                <xsd:enumeration value="An error occurred saving answers." />
                <xsd:enumeration value="Survey requires a unique identity." />
                <xsd:enumeration value="Improperly configured survey." />
                <xsd:enumeration value="Survey not found." />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="surveyId" type="xsd:string" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="SurveyAnswerSet">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="answer" type="survey:SurveyQuestionAnswer" />
        </xsd:sequence>
        <xsd:attribute name="surveyId" type="xsd:string" use="required" />
      </xsd:complexType>
      <xsd:complexType name="SurveyQuestionAnswer">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="choiceId" type="xsd:string" />
        </xsd:sequence>
        <xsd:attribute name="questionId" type="xsd:string" use="required" />
      </xsd:complexType>
      <xsd:element name="survey" type="survey:Survey" />
      <xsd:element name="surveyAnswerSet" type="survey:SurveyAnswerSet" />
    </xsd:schema>
    <xsd:schema xmlns:identity="http://ns.lexisnexis.com/identity-proofing/1.0" xmlns:survey="http://ns.lexisnexis.com/survey/1.0" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://ns.lexisnexis.com/identity-proofing/1.0" version="9.0">
      <xsd:import namespace="http://ns.lexisnexis.com/survey/1.0" />
      <xsd:complexType name="IdentityProofingRequest">
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="1" name="workFlow" type="xsd:string" />
          <xsd:choice>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="inputSubject" type="identity:InputSubject" />
            <xsd:element minOccurs="0" maxOccurs="1" name="scoreRequest" type="identity:IdentityScoreRequest" />
            <xsd:element minOccurs="0" maxOccurs="1" name="surveySaveRequest" type="survey:SurveyAnswerSet" />
          </xsd:choice>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="RequestInfo" />
          <xsd:element minOccurs="0" maxOccurs="1" name="inputOptions" type="identity:InputOptions" />
        </xsd:sequence>
        <xsd:attribute name="transactionID" type="identity:TransactionID" use="required" />
        <xsd:attribute name="locale" type="xsd:language" use="optional" />
        <xsd:attribute name="customerReference" type="xsd:string" use="optional" />
        <xsd:attribute name="source" type="xsd:string" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="RequestInfo">
        <xsd:sequence>
          <xsd:any minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
        <xsd:attribute name="type" type="identity:InfoType" use="optional" />
      </xsd:complexType>
      <xsd:simpleType name="InfoType">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="InstantID" />
          <xsd:enumeration value="OTHER" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="InputSubject">
        <xsd:sequence>
          <xsd:choice>
            <xsd:element minOccurs="0" maxOccurs="1" name="person" type="identity:InputPerson" />
            <xsd:element minOccurs="0" maxOccurs="1" name="organization" type="identity:InputOrganization" />
          </xsd:choice>
          <xsd:element minOccurs="0" maxOccurs="1" name="options" type="identity:SubjectOptions">
            <xsd:unique name="questionReferenceQuestionCodeKey">
              <xsd:selector xpath="identity:QuestionReference" />
              <xsd:field xpath="@questionCode" />
            </xsd:unique>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="InputRelatedSubject">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:complexContent mixed="false">
          <xsd:extension base="identity:InputSubject">
            <xsd:attribute name="subjectRelationship" type="identity:SubjectRelationshipTypeEnum" use="optional" />
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:simpleType name="SubjectRelationshipTypeEnum">
        <xsd:restriction base="xsd:token">
          <xsd:enumeration value="SPOUSE" />
          <xsd:enumeration value="CHILD" />
          <xsd:enumeration value="PARENT" />
          <xsd:enumeration value="GUARDIAN" />
          <xsd:enumeration value="GRANDPARENT" />
          <xsd:enumeration value="EMPLOYEE" />
          <xsd:enumeration value="EMPLOYER" />
          <xsd:enumeration value="CUSTOMER" />
          <xsd:enumeration value="RELATIVE" />
          <xsd:enumeration value="ROOMMATE" />
          <xsd:enumeration value="NEIGHBOR" />
          <xsd:enumeration value="PROPERTY_SELLER" />
          <xsd:enumeration value="PROPERTY_BUYER" />
          <xsd:enumeration value="AUTHORIZED_REPRESENTATIVE" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="InputPerson">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="name" type="identity:InputName" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="aka" type="identity:InputName" />
          <xsd:element minOccurs="0" maxOccurs="1" name="LexID" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="ssn" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="dateOfBirth" type="identity:Date" />
          <xsd:element minOccurs="0" maxOccurs="1" name="dateOfDeath" type="identity:Date" />
          <xsd:element minOccurs="0" maxOccurs="1" name="gender" type="identity:Gender" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="address" type="identity:InputAddress" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="phone" type="identity:InputPhone" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="email" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="ipAddress" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="taxId" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="upin" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="npi" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="license" type="identity:InputLicense" />
          <xsd:element minOccurs="0" maxOccurs="1" name="passport" type="identity:Passport" />
          <xsd:element minOccurs="0" maxOccurs="1" name="schools" type="identity:Schools" />
          <xsd:element minOccurs="0" maxOccurs="1" name="accounts" type="identity:InputAccounts" />
          <xsd:element minOccurs="0" maxOccurs="1" name="velocityAccounts" type="identity:VelocityAccounts" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="vehicle" type="identity:VehicleInfo" />
          <xsd:element minOccurs="0" maxOccurs="1" name="customerSubjectID" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="relatedSubject" type="identity:InputRelatedSubject" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="InputName">
        <xsd:all>
          <xsd:element minOccurs="0" name="prefix" type="xsd:string" />
          <xsd:element minOccurs="0" name="first" type="xsd:string" />
          <xsd:element minOccurs="0" name="middle" type="xsd:string" />
          <xsd:element minOccurs="0" name="last" type="xsd:string" />
          <xsd:element minOccurs="0" name="suffix" type="xsd:string" />
        </xsd:all>
      </xsd:complexType>
      <xsd:complexType name="InputOrganization">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="companyName" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="address" type="identity:InputAddress" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="phone" type="identity:InputPhone" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="email" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="taxId" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="upin" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="npi" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="license" type="identity:InputLicense" />
          <xsd:element minOccurs="0" maxOccurs="1" name="account" type="identity:InputAccount" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="vehicle" type="identity:VehicleInfo" />
          <xsd:element minOccurs="0" maxOccurs="1" name="customerSubjectID" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="relatedSubject" type="identity:InputRelatedSubject" />
        </xsd:sequence>
        <xsd:attribute name="entityType" type="xsd:string" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="InputAddress">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="addressLine1" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="addressLine2" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="city" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="stateCode" type="identity:StateCode" />
          <xsd:element minOccurs="0" maxOccurs="1" name="zip5" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="countryCode" type="xsd:string" />
        </xsd:sequence>
        <xsd:attribute name="addressPurpose" type="identity:AddressPurposeType" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="InputOptions">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="inputOption" type="identity:Extrinsic" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SubjectOptions">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
				Options related to an input subject.
			</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="questionReference" type="identity:QuestionReference" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="inputOption" type="identity:Extrinsic" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="QuestionReference">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
				Defines user supplied data to be used in generating an authentication quiz.
				The questionCode attribute should be unique and defines the quiz question that will be generated.
				Each data element will contain an answer for the question.  A question will be generated for each instance of data.
				Each exclusion element will contain a value which must be excluded from the wrong answer set.
				Each variable element can be used to replace parameters in the question text.
			</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="unbounded" name="data" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="exclusion" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="variable" type="identity:Extrinsic" />
        </xsd:sequence>
        <xsd:attribute name="questionCode" type="xsd:string" use="required" />
      </xsd:complexType>
      <xsd:complexType name="Schools">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="school" type="identity:School" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="School">
        <xsd:all>
          <xsd:element minOccurs="0" name="name" type="xsd:string" />
          <xsd:element minOccurs="0" name="graduationYear" type="xsd:int" />
        </xsd:all>
      </xsd:complexType>
      <xsd:complexType name="VehicleInfo">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="VIN" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="modelYear" type="xsd:int" />
          <xsd:element minOccurs="0" maxOccurs="1" name="make" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="model" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="majorColor" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="minorColor" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="type" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="style" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="titleInfo" type="identity:TitleInfo" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="TitleInfo">
        <xsd:all>
          <xsd:element minOccurs="0" maxOccurs="1" name="number" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="issueDate" type="identity:Date" />
          <xsd:element minOccurs="0" maxOccurs="1" name="expirationDate" type="identity:Date" />
          <xsd:element minOccurs="0" maxOccurs="1" name="state" type="xsd:string" />
        </xsd:all>
      </xsd:complexType>
      <xsd:complexType name="Passport">
        <xsd:all>
          <xsd:element minOccurs="0" maxOccurs="1" name="number" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="issueDate" type="identity:Date" />
          <xsd:element minOccurs="0" maxOccurs="1" name="expirationDate" type="identity:Date" />
          <xsd:element minOccurs="0" maxOccurs="1" name="country" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="machineReadableLine1" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="machineReadableLine2" type="xsd:string" />
        </xsd:all>
      </xsd:complexType>
      <xsd:complexType name="InputPhone">
        <xsd:annotation>
          <xsd:documentation />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="phoneNumber" type="xsd:string" />
        </xsd:sequence>
        <xsd:attribute name="phonePurpose" type="xsd:string" use="optional" />
      </xsd:complexType>
      <xsd:simpleType name="LicenseType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="DRIVERS_LICENSE" />
          <xsd:enumeration value="PROFESSIONAL_LICENSE" />
          <xsd:enumeration value="DEA_LICENSE" />
          <xsd:enumeration value="MEDICAL_LICENSE" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="InputLicense">
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="1" name="licenseNumber" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="issuer" type="xsd:string" />
        </xsd:sequence>
        <xsd:attribute name="type" type="identity:LicenseType" use="required" />
        <xsd:attribute name="sequence" type="xsd:int" use="optional" />
      </xsd:complexType>
      <xsd:simpleType name="AccountType">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="CREDIT" />
          <xsd:enumeration value="DEBIT" />
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="VelocityAccounts">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="alias" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="accountId" type="xsd:string" />
        </xsd:sequence>
        <xsd:attribute name="type" type="identity:AccountType" />
      </xsd:complexType>
      <xsd:complexType name="InputAccounts">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="inputAccount" type="identity:InputAccount" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="InputAccount">
        <xsd:sequence>
          <xsd:element name="accountNumber" type="xsd:string" />
        </xsd:sequence>
        <xsd:attribute name="type" type="identity:AccountType" />
      </xsd:complexType>
      <xsd:complexType name="IdentityScoreRequest">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="quizId" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="answer" type="identity:IdentityQuizAnswer" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="IdentityQuizAnswer">
        <xsd:sequence>
          <xsd:element name="questionId" type="xsd:string" />
          <xsd:element name="choiceId" type="xsd:string" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="IdentityProofingResponse">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="workFlow" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="status" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="statusDescription" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="productResponse" type="identity:ProductResponse" />
          <xsd:element minOccurs="0" maxOccurs="1" name="responseCodes" type="identity:ResponseCodes" />
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="identityProofingException" type="identity:IdentityProofingException" />
        </xsd:sequence>
        <xsd:attribute name="transactionID" type="xsd:string" />
        <xsd:attribute name="requestId" type="xsd:string" use="optional" />
        <xsd:attribute name="locale" type="xsd:language" use="optional" />
        <xsd:attribute name="responseUniqueId" type="xsd:string" use="optional" />
        <xsd:attribute name="parentResponseId" type="xsd:string" use="optional" />
        <xsd:attribute name="customerReference" type="xsd:string" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="IdentityProofingException">
        <xsd:all>
          <xsd:element minOccurs="0" name="code" type="xsd:string" />
          <xsd:element minOccurs="0" name="source" type="xsd:string" />
          <xsd:element minOccurs="0" name="message" type="xsd:string" />
        </xsd:all>
      </xsd:complexType>
      <xsd:complexType name="ProductResponse" abstract="true">
        <xsd:annotation>
          <xsd:documentation xml:lang="en" />
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="responseCodes" type="identity:ResponseCodes" />
          <xsd:element minOccurs="0" maxOccurs="1" name="extrinsics" type="identity:Extrinsics" />
        </xsd:sequence>
        <xsd:attribute name="sequence" type="xsd:int" use="optional" />
        <xsd:attribute name="LexID" type="xsd:string" use="optional" />
      </xsd:complexType>
      <xsd:complexType name="ResponseCode">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="code" type="xsd:string" />
          <xsd:element minOccurs="0" maxOccurs="1" name="description" type="xsd:string" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="ResponseCodes">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="responseCode" type="identity:ResponseCode" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="GenericProductResponse">
        <xsd:complexContent mixed="false">
          <xsd:extension base="identity:ProductResponse">
            <xsd:sequence>
              <xsd:element minOccurs="0" maxOccurs="1" name="responseInfo" type="identity:ResponseInfo" />
            </xsd:sequence>
            <xsd:attribute name="type" type="identity:InfoType" use="optional" />
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="DiscoveryResponse">
        <xsd:complexContent mixed="false">
          <xsd:extension base="identity:ProductResponse">
            <xsd:sequence>
              <xsd:element minOccurs="1" maxOccurs="1" name="status" type="identity:DiscoveryProductStatus" />
              <xsd:element minOccurs="0" maxOccurs="1" name="recordCount" type="xsd:int" />
              <xsd:choice>
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="person" type="identity:PersonSubject" />
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="organization" type="identity:OrganizationSubject" />
              </xsd:choice>
              <xsd:element minOccurs="0" maxOccurs="1" name="responseInfo" type="identity:ResponseInfo" />
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="ResponseInfo">
        <xsd:sequence>
          <xsd:any minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="InstantVerifyResponse">
        <xsd:complexContent mixed="false">
          <xsd:extension base="identity:ProductResponse">
            <xsd:sequence>
              <xsd:element minOccurs="1" maxOccurs="1" name="status" type="identity:InstantVerifyProductStatus" />
